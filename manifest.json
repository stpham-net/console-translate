{
  "name": "Console Translate",
  "version": "1.1",
  "description": "Console Translate by Stpham",
  "author": "Stpham, Inc.",
  "homepage_url": "https://www.stpham.net/",
  "icons": {
    "16": "icon_true.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "jquery-ui.min.css",
        "style.css"
      ],
      "js": [
        "jquery.min.js",
        "jquery-ui.min.js",
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "Console Translate"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "images/*"
  ]
}